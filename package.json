        "typescript": "^5.2.2",
        "webpack": "^5.89.0"
    },
    "files": [
        "build/es2019/",
        "build/es5/",
        "src/"
    ],
    "homepage": "https://github.com/chrisguttandin/timing-object",
    "keywords": [
        "Timing Object",
        "Timing Provider",
        "synchronisation",
        "timing",
        "timingsrc",
